<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.gonicollazo.raulTarea5.Models">

	<class name="Patient" table="Patient">
		<id name="id" column="idPatient">
			<generator class="identity" />
		</id>
		<property name="name" />
		<property name="sex" />
		<list name="doctor" constrained="true"">
			<key column="idpatient" />
			<index column="doctor_order" />
			<one-to-many class="Doctor" />
		</list>

	</class>

</hibernate-mapping>